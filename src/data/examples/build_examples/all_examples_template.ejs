---
title: examples
slug: examples/
---

<div id="learn-page">

  {{> sidebar}}

  <div class="column-span">

    <section>
      <h2>{{#i18n "Examples"}}{{/i18n}}</h2>

      <div class="column_0">
      <% var t = 0; var col=1; for(var topic in all['examples']) { %>
        <p name='<%=topic.toLowerCase()%>' class='anchor'>
        <strong><% var newt = topic.replace('-', ' ')%><%=newt%></strong><br>
        <% for(var i=0; i<all['examples'][topic].length; i++) { %>
          <a href="<%=all['examples'][topic][i][1]%>"><%= all['examples'][topic][i][0] %></a><br>
          <% t++ %>
        <% } %>
        </p>
        <% if (t >= total['examples']/3) { %>
          </div>
          <div class="column_<%=col%>">
        <% col++; t=0; } %>
      <% } %>
      </div>

      <div class="spacer"></div>

      <a name="demos" class="anchor"><h3>Demos</h3></a>
      <div class="column_0">
      <% var t = 0; var col=1; for(var topic in all['demos']) { %>
        <p>
        <strong><% var newt = topic.replace('-', ' ')%><%=newt%></strong><br>
        <% for(var i=0; i<all['demos'][topic].length; i++) { %>
          <a href="<%=all['demos'][topic][i][1]%>"><%= all['demos'][topic][i][0] %></a><br>
          <% t++ %>
        <% } %>
        </p>
        <% if (t >= total['demos']/3) { %>
          </div>
          <div class="column_<%=col%>">
        <% col++; t=0; } %>
      <% } %>
      </div>
    </section>

    {{> footer}}

  </div> <!-- end column-span -->

  {{> asterisk}}

</div><!-- end id="get-started-page"  -->
